<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    tools:context=".tabs.PersonalChats">

    <com.uyscuti.social.chatsuit.dialogs.DialogsList
        android:id="@+id/dialogsList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:scrollbars="none"
        android:paddingTop="0dp"
        android:paddingBottom="0dp"
        app:dialogAvatarSize="52dp"
        app:dialogAvatarWidth="52dp"
        app:dialogAvatarHeight="52dp"
        app:dialogDividerEnabled="false"
        app:dialogItemPadding="16dp"
        app:dialogMessageTextColor="#8696A0"
        app:dialogMessageTextSize="15sp"
        app:dialogMessageTextStyle="normal"
        app:dialogTitleTextColor="#000000"
        app:dialogTitleTextSize="17sp"
        app:dialogTitleTextStyle="normal"
        app:dialogDateTextColor="#8696A0"
        app:dialogDateTextSize="13sp"
        app:dialogDateStyle="normal"
        app:dialogUnreadBubbleEnabled="true"
        app:dialogUnreadBubbleBackgroundColor="#25D366"
        app:dialogUnreadBubbleTextColor="#FFFFFF"
        app:dialogUnreadBubbleTextSize="12sp"
        app:dialogUnreadBubbleTextStyle="bold"
        app:dialogUnreadItemBackground="#FFFFFF"
        app:dialogUnreadTitleTextStyle="bold"
        app:dialogUnreadTitleTextColor="#000000"
        app:dialogUnreadMessageTextStyle="normal"
        app:dialogUnreadMessageTextColor="#000000"
        app:dialogUnreadDateColor="#25D366"
        app:dialogUnreadDateStyle="normal"
        app:dialogAvatarBorderColor="@android:color/transparent"
        app:dialogAvatarBorderWidth="0dp"
        app:dialogItemBackground="#FFFFFF"
        app:dialogItemBackgroundColor="#FFFFFF"
        app:dialogItemSelectedBackgroundColor="#F7F8FA"
        app:dialogMessageAvatarEnabled="false" />

</FrameLayout>