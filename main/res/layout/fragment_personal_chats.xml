<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    tools:context=".tabs.PersonalChats">

    <com.uyscuti.social.chatsuit.dialogs.DialogsList
        android:id="@+id/dialogsList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:scrollbars="none"
        app:dialogAvatarSize="49dp"
        app:dialogAvatarWidth="49dp"
        app:dialogAvatarHeight="49dp"
        app:dialogDividerEnabled="true"
        app:dialogDividerColor="#E9EDEF"
        app:dialogDividerLeftPadding="72dp"
        app:dialogDividerRightPadding="0dp"
        app:dialogItemPadding="10dp"
        app:dialogMessageTextColor="#667781"
        app:dialogMessageTextSize="14sp"
        app:dialogMessageTextStyle="normal"
        app:dialogTitleTextColor="#111B21"
        app:dialogTitleTextSize="16sp"
        app:dialogTitleTextStyle="normal"
        app:dialogDateTextColor="#667781"
        app:dialogDateTextSize="12sp"
        app:dialogDateStyle="normal"
        app:dialogUnreadBubbleEnabled="true"
        app:dialogUnreadBubbleBackgroundColor="#25D366"
        app:dialogUnreadBubbleTextColor="#FFFFFF"
        app:dialogUnreadBubbleTextSize="11sp"
        app:dialogUnreadBubbleTextStyle="bold"
        app:dialogUnreadItemBackground="#FFFFFF"
        app:dialogUnreadTitleTextStyle="bold"
        app:dialogUnreadTitleTextColor="#000000"
        app:dialogUnreadMessageTextStyle="bold"
        app:dialogUnreadMessageTextColor="#000000"
        app:dialogUnreadDateColor="#25D366"
        app:dialogUnreadDateStyle="normal"
        app:dialogAvatarBorderColor="@android:color/transparent"
        app:dialogAvatarBorderWidth="0dp"
        app:dialogItemBackground="#FFFFFF"
        app:dialogItemBackgroundColor="#FFFFFF"
        app:dialogItemSelectedBackgroundColor="#F0F2F5"
        app:dialogMessageAvatarEnabled="false" />

</FrameLayout>